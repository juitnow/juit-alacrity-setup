{
  "name": "@juit/alacrity-setup",
  "description": "A little web interface to configure Alacrity barcode scanners",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "preview": "vite preview",
    "updates": "rm -rf node_modules package-lock.json && npx @juit/check-updates && npm install",
    "check": "vue-tsc --noEmit && eslint src"
  },
  "dependencies": {
    "@juit/qrcode": "^1.0.82",
    "@quasar/extras": "^1.17.0",
    "quasar": "^2.18.6",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@plugjs/eslint-plugin-vue": "^0.1.23",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/node": "<21",
    "@types/web-bluetooth": "^0.0.21",
    "@vitejs/plugin-vue": "^6.0.4",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  },
  "license": "Apache-2.0",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juitnow/juit-alacrity-setup.git"
  },
  "keywords": [
    "alacrity",
    "barcode",
    "configuration"
  ],
  "author": "Juit Developers <developers@juit.com>",
  "bugs": {
    "url": "https://github.com/juitnow/juit-alacrity-setup/issues"
  },
  "homepage": "https://github.com/juitnow/juit-alacrity-setup#readme"
}
