{
  "name": "@juit/alacrity-setup",
  "description": "A little web interface to configure Alacrity barcode scanners",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "preview": "vite preview",
    "updates": "rm -rf node_modules package-lock.json && npx @juit/check-updates && npm install",
    "check": "vue-tsc --noEmit && eslint src"
  },
  "dependencies": {
    "@juit/qrcode": "^1.0.22",
    "@quasar/extras": "^1.16.13",
    "quasar": "^2.17.2",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@plugjs/eslint-plugin": "^0.2.25",
    "@quasar/vite-plugin": "^1.8.1",
    "@types/node": "<21",
    "@types/web-bluetooth": "^0.0.20",
    "@vitejs/plugin-vue": "^5.2.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.31.0",
    "sass": "^1.80.7",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  },
  "license": "Apache-2.0",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juitnow/juit-alacrity-setup.git"
  },
  "keywords": [
    "alacrity",
    "barcode",
    "configuration"
  ],
  "author": "Juit Developers <developers@juit.com>",
  "bugs": {
    "url": "https://github.com/juitnow/juit-alacrity-setup/issues"
  },
  "homepage": "https://github.com/juitnow/juit-alacrity-setup#readme"
}
